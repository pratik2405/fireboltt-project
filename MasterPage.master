    <asp:ContentPlaceHolder id="head" runat="server">
    </asp:ContentPlaceHolder>
    <style type="text/css">
        .style1
        {
            
            height: 78px;
            width: 100%;
        }
        .style2
        {
            height: 78px;
            width: 49px;
        }
        .style3
        {
            width: 132px;
            height: 77px;
            margin-top: 0px;
        }
        .style4
        {
            width: 391px;
            height: 69px;
            margin-left: 0px;
        }
    </style>
</head>
<body>
    <form id="form1" runat="server">
    <div>
        <asp:ContentPlaceHolder id="ContentPlaceHolder1" runat="server">
        
